        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/consultBtn"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:src="@drawable/ic_008_stethoscope"
            app:backgroundTint="@color/bottomgreen"
            app:fabCustomSize="70dp"
            app:layout_anchor="@id/bottomNavigationItemView"
            app:maxImageSize="40dp"
            app:tint="@color/colorAccent" />

        <com.google.android.material.bottomappbar.BottomAppBar
            android:id="@+id/bottomNavigationItemView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:backgroundTint="@color/colorAccent"
            app:fabAlignmentMode="center"
            app:fabCradleRoundedCornerRadius="30dp"
            app:hideOnScroll="false"
            app:menu="@menu/bottom_bar"
            app:navigationIcon="@drawable/ic_018_hospital">

        </com.google.android.material.bottomappbar.BottomAppBar>


    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</androidx.constraintlayout.widget.ConstraintLayout>